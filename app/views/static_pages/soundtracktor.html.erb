<script>
	$(function() {
		$.getJSON('http://www.highcharts.com/samples/data/jsonp.php?filename=aapl-c.json&callback=?', function(data) {

			// Create the chart
			$('#container').highcharts('StockChart', {
				

				rangeSelector : {
					selected : 1
				},

				title : {
					text : 'Soundtracktor ApS'
				},
				
				series : [{
					name : 'Soundtracktor kurs',
					data : data,
					marker : {
						enabled : true,
						radius : 3
					},
					shadow : true,
					tooltip : {
						valueDecimals : 2
					}
				}]
			});
		});
	});
</script>


