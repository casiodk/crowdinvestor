<div class="row company-progress-bar">
	<div class="col-md-12">
		<div class="progress">
			<div class="progress-bar progress-bar-danger" style="width: 20%">
			  <span class="sr-only">20% Complete (danger)</span>
			</div>
			<div class="progress-bar progress-bar-warning" style="width: 30%">
			  <span class="sr-only">30% Complete (warning)</span>
			</div>
		  <div class="progress-bar progress-bar-success" style="width: 25%">
		    <span class="sr-only">25% Complete (success)</span>
		  </div>
		</div>
	</div>
</div>

<div class="row company-usp">
	<div class="col-md-3">
		<div class="well">
			<div class="row">
				<div class="col-md-3">
					<%= fa_icon "globe 3x" %>
				</div>
				<div class="col-md-9">
					<h4>Worldwide</h4>
				</div>
			</div>
		</div>
	</div>

	<div class="col-md-3">
		<div class="well">
			<div class="row">
				<div class="col-md-3">
					<%= fa_icon "group 3x" %>
				</div>
				<div class="col-md-9">
					<h4>5 investorer</h4>
				</div>
			</div>
		</div>
	</div>

	<div class="col-md-3">
		<div class="well">
			<div class="row">
				<div class="col-md-3">
					<%= fa_icon "home 3x" %>
				</div>
				<div class="col-md-9">
					<h4>Værdi 5.8 mio</h4>
				</div>
			</div>
		</div>
	</div>

	<div class="col-md-3">
		<div class="well">
			<div class="row">
				<div class="col-md-3">
					<%= fa_icon "home 3x" %>
				</div>
				<div class="col-md-9">
					<h4>Værdi 5.8 mio</h4>
				</div>
			</div>
		</div>
	</div>
</div>

<div class="row">
	<div class="col-md-3">
		<div class="well">
			<%= render "companies/executive_summary_list", company_name: @company_name, executive_summary_points: @executive_summary_points %>
			<%= render "companies/rewards" %>
		</div>
	</div>

	<div class="col-md-9">
		<div class="row">
			<div class="col-md-8">
				<iframe src="//player.vimeo.com/video/76625644" width="100%" height="281" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
			</div>
			<div class="col-md-3 col-md-offset-1">
				<br><br><br><br><br><br><br><br><br><br>
				<%= link_to "Invester nu", "#", class: "btn btn-lg btn-success", role: "button" %>
			</div>
		</div>

		<div class="row">
			<div class="col-md-12">
				<ul class="nav nav-tabs">
				  <li class="active">
				  	<a href="#executive-summary" data-toggle="tab">Executive summary</a>
				  </li>
				  <li>
				  	<a href="#rate_development" data-toggle="tab">Kursudvikling</a>
				  </li>
				  <li>
				  	<a href="#order_depth" data-toggle="tab">Ordredybde</a>
				  </li>
				  <li>
				  	<a href="#ownership" data-toggle="tab">Ejerandele</a>
				  </li>
				  <li>
				  	<%= link_to "Q&A (4)", "#q_and_a", data: { toggle: "tab" } %>
				  </li>
				  <li>
				  	<%= link_to "Blog", "#blog", data: { toggle: "tab" } %>
				  </li>
				</ul>

				<div class="tab-content">
				  <div class="tab-pane fade in active" id="executive-summary">
				  	<%= render 'companies/executive_summary' %>
				  </div>
				  <div class="tab-pane fade" id="rate_development">
			  		<%= render 'companies/graphs/rate_development' %>
				  </div>
				  <div class="tab-pane fade" id="order_depth">
				  	 <%= render 'companies/graphs/order_depth' %>
				  </div>
				  <div class="tab-pane fade" id="ownership">
				  	 <%= render 'companies/graphs/ownership' %>
				  </div>
				  <div class="tab-pane fade" id="q_and_a">
				  	 <%= render 'companies/q_and_a' %>
				  </div>
				  <div class="tab-pane fade" id="blog">

				  </div>
				</div>
			</div>
		</div>
		<hr>
	</div>
</div>

<!-- Social -->
